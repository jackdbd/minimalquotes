{
  "name": "minimalquotes",
  "version": "1.0.0",
  "description": "A minimal app for inspirational quotes",
  "main": "index.js",
  "author": "Giacomo Debidda <jackdebidda@gmail.com>",
  "license": "MIT",
  "scripts": {
    "build": "yarn clean && yarn build:css && shadow-cljs release app",
    "build:css": "tailwindcss build src/assets/css/styles.css -o public/css/output.css",
    "cards": "shadow-cljs watch cards",
    "clean": "rimraf out public/js",
    "deploy": "yarn build && firebase deploy",
    "deploy:rules": "firebase deploy --only firestore:rules",
    "dev": "shadow-cljs watch app",
    "e2e:css": "node e2e/unused-css.js",
    "e2e:format": "prettier --write 'e2e/*.js'",
    "e2e:login": "node e2e/login-user.js",
    "e2e:perf": "node e2e/performance.js",
    "e2e:screenshot": "node e2e/make-screenshot-after-hover-on-tag.js",
    "emulators": "firebase emulators:start",
    "format": "zprint-clj --hang --input \"./src/**/*.{clj,cljs,cljc,edn}\" -o ./",
    "lint": "clj-kondo --lint src",
    "report": "shadow-cljs run shadow.cljs.build-report app out/report.html && http-server out -p 8090",
    "server": "shadow-cljs stop && shadow-cljs start",
    "test": "shadow-cljs compile test && node out/test.js",
    "test:watch": "shadow-cljs watch test --config-merge \"{:autorun true}\""
  },
  "dependencies": {
    "create-react-class": "^15.6.3",
    "firebase": "^7.16.0",
    "firebaseui": "^4.6.0",
    "react": "^16.13.1",
    "react-bootstrap-typeahead": "^5.1.0",
    "react-dom": "^16.13.1",
    "tailwindcss": "^1.4.6"
  },
  "devDependencies": {
    "@testing-library/dom": "^7.20.2",
    "@testing-library/react": "^10.4.6",
    "clj-kondo": "^2020.1.13",
    "firebase-tools": "^8.5.0",
    "http-server": "^0.12.3",
    "jsdom": "^16.3.0",
    "jsdom-global": "^3.0.2",
    "prettier": "^2.0.5",
    "rimraf": "^3.0.2",
    "shadow-cljs": "^2.10.14",
    "taiko": "^1.0.15",
    "taiko-diagnostics": "^0.4.0",
    "zprint-clj": "^0.8.0"
  }
}
